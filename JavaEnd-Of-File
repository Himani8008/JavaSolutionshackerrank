import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        int i = 1;
        Scanner sc = new Scanner(System.in);
        while(sc.hasNext()){
        //ScannerName.hasNextLine() method returns true if there is another line in the input of this scanner. 
            String hi = sc.nextLine();
            System.out.println(i + " " + hi);
            i++;
        }
    }
}
